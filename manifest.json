{
  "manifest_version": 2,

  "name": "HTML5 Video Speed Control",
  "description": "Speed controls for html5 video.",
  "options_page": "options.html",
  "version": "1.43",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "css": ["style.css"],
      "js": ["speedcontrol.js"],
      "all_frames": true
    }
  ]
}
